<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app= "http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/y160"
    android:orientation="vertical"
    >


    <android.support.constraint.ConstraintLayout
        android:id="@+id/con_lay_phone"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#fefefe"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="电话："
            android:alpha="0.54"
            android:textSize="@dimen/text_size_3"
            app:layout_constraintLeft_toLeftOf="@+id/tv_myphone"
            android:layout_marginLeft="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/tv_myphone" />
        <ImageView
            android:layout_width="@dimen/ic_size_min"
            android:layout_height="@dimen/ic_size_min"
            android:background="@drawable/ic_phone"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toTopOf="@+id/view6"
            android:id="@+id/iv_phone"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="15870617281"
            android:alpha="0.87"
            android:textSize="@dimen/text_size_2"
            app:layout_constraintLeft_toRightOf="@+id/iv_phone"
            android:layout_marginLeft="16dp"
            android:id="@+id/tv_myphone"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" />
        <View
            android:layout_width="395dp"
            android:layout_height="1dp"
            android:background="@color/bg_lin_huise"
            android:alpha="0.87"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="@dimen/x96"
            app:layout_constraintLeft_toLeftOf="parent"
            android:id="@+id/view6"
            android:layout_marginStart="@dimen/x96"
            android:layout_marginEnd="8dp" />


    </android.support.constraint.ConstraintLayout>


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#fefefe"
        android:id="@+id/con_lay_qq"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="QQ："
            android:alpha="0.54"
            android:textSize="@dimen/text_size_3"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/tv_myqq"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/tv_myqq"
            app:layout_constraintVertical_bias="0.666" />
        <ImageView
            android:layout_width="@dimen/ic_size_min"
            android:layout_height="@dimen/ic_size_min"
            android:background="@drawable/qq"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:id="@+id/iv_ic_qq"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.454" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="421070718"
            android:alpha="0.87"
            android:textSize="@dimen/text_size_2"
            app:layout_constraintLeft_toRightOf="@+id/iv_ic_qq"
            android:layout_marginLeft="16dp"
            android:id="@+id/tv_myqq"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/view7" />
        <View
            android:layout_width="395dp"
            android:layout_height="1dp"
            android:background="@color/bg_lin_huise"
            android:alpha="0.87"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="@dimen/x96"
            app:layout_constraintLeft_toLeftOf="parent"
            android:id="@+id/view7"
            android:layout_marginStart="@dimen/x96"
            android:layout_marginEnd="8dp" />

        <ImageView
            android:layout_width="@dimen/y20"
            android:layout_height="@dimen/y20"
            android:background="@drawable/ic_qq_erweima"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="8dp"
            android:id="@+id/iv_qq_erweima"
            android:alpha="0.7"
            app:layout_constraintBottom_toTopOf="@+id/view7"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#fefefe"
        android:id="@+id/con_lay_wx"
        >
        <ImageView
            android:layout_width="@dimen/ic_size_min"
            android:layout_height="@dimen/ic_size_min"
            android:background="@drawable/ic_wx"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:id="@+id/iv_wx"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="16dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="wx421070718"
            android:alpha="0.87"
            android:textSize="@dimen/text_size_2"
            app:layout_constraintLeft_toRightOf="@+id/iv_wx"
            android:layout_marginLeft="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            android:id="@+id/tv_mywx"
            android:layout_marginStart="16dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="微信号："
            android:alpha="0.54"
            android:textSize="@dimen/text_size_3"
            app:layout_constraintLeft_toLeftOf="@+id/tv_mywx"
            android:layout_marginLeft="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:id="@+id/textView"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/tv_mywx" />
        <ImageView
            android:layout_width="@dimen/y20"
            android:layout_height="@dimen/y20"
            android:background="@drawable/ic_wx_weweima"
            android:alpha="0.7"
            android:id="@+id/iv_wx_erweima"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" />

        <View
            android:layout_width="395dp"
            android:layout_height="1dp"
            android:background="@color/bg_lin_huise"
            android:alpha="0.87"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="@dimen/x96"
            app:layout_constraintLeft_toLeftOf="parent"
            android:id="@+id/view8"
            android:layout_marginStart="@dimen/x96"
            android:layout_marginEnd="8dp" />

    </android.support.constraint.ConstraintLayout>


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#fefefe"
        >
        <ImageView
            android:layout_width="@dimen/ic_size_min"
            android:layout_height="@dimen/ic_size_min"
            android:background="@drawable/e_mail"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:id="@+id/iv_e_mail"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="16dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="421070718@qq.com"
            android:alpha="0.87"
            android:textSize="@dimen/text_size_2"

            app:layout_constraintLeft_toRightOf="@+id/iv_e_mail"
            android:layout_marginLeft="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            android:id="@+id/tv_myemail"
            android:layout_marginStart="16dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="e-mail："
            android:alpha="0.54"
            android:textSize="@dimen/text_size_3"
            app:layout_constraintLeft_toLeftOf="@+id/tv_myemail"
            android:layout_marginLeft="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/tv_myemail" />
    </android.support.constraint.ConstraintLayout>



</LinearLayout>
